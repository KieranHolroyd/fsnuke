{
  "name": "@ndkieran/nodekill",
  "description": "Remove all node_modules recursively within child directories.",
  "version": "0.1.0",
  "author": "Kieran Holroyd",
  "bin": {
    "nodekill": "./bin/run"
  },
  "bugs": "https://github.com/kieranholroyd/nodekill/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/plugin-help": "^3.2.0",
    "@types/node": "^17.0.8",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "fast-folder-size": "^1.6.1",
    "globby": "^10.0.2",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "tslib": "^1.13.0",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/kieranholroyd/nodekill",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "nodekill",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "kieranholroyd/nodekill",
  "scripts": {
    "start": "node ./bin/run",
    "dev": "tsc -w",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest",
    "test": "echo NO TESTS",
    "oc::manifest": "oclif-dev manifest",
    "oc::readme": "oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
